<Statements>
<Statement>CREATE TABLE [SupplierAttraction] ([SupplierId] smallint NOT NULL , [AttractionId] smallint NOT NULL , [SupplierAttractionDate] datetime NOT NULL , PRIMARY KEY([SupplierId], [AttractionId])) </Statement>
<Statement>CREATE NONCLUSTERED INDEX [ISUPPLIERATTRACTION1] ON [SupplierAttraction] ([AttractionId] )</Statement>
<Statement>INSERT INTO [SupplierAttraction] ([SupplierId], [AttractionId], [SupplierAttractionDate]) SELECT [SupplierId], [AttractionId], convert( DATETIME, '17530101', 112 ) FROM [Attractions] T  WHERE NOT EXISTS (SELECT 1 FROM [SupplierAttraction] WHERE SupplierId= T.SupplierId AND AttractionId= T.AttractionId)</Statement>
